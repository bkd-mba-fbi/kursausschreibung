{{#event-list-search queryChanged=(action "queryChanged") events=this.events as |filteredEvents|}}
  {{#list-pagination page=this.page items=filteredEvents route=this.route as |eventsOnCurrentPage|}}
    {{#each eventsOnCurrentPage as |event|}}
      {{event-list-item event=event}}
    {{/each}}
  {{/list-pagination}}
{{/event-list-search}}
